		"custom": {
			"color": {
				"foreground": "var(--wp--preset--color--black)",
				"background": "var(--wp--preset--color--white)",
				"interactive": "var(--wp--preset--color--primary)",
				"interactiveHover": "var(--wp--preset--color--primary-dark)",
				"uiBorder": "var(--wp--preset--color--gray-500)",
				"warn": "#EB7100",
				"error": "#D70404",
				"focus": "var(--wp--preset--color--primary-light)"
			},
			"alignment": {
				"alignedMaxWidth": "50%"
			},
			"animation":{
				"transitionDuration": "0.3s",
				"transitionType": "all"
			},
			"button": {
				"active": {
					"color": {
						"background": "var(--wp--custom--color--interactive)",
						"text": "var(--wp--custom--color--background)"
					}
				},
				"focus": {
					"color": {
						"background": "var(--wp--custom--color--interactive-hover)",
						"text": "var(--wp--custom--color--background)"
					},
					"outline": {
						"color": "var(--wp--custom--color--focus)",
						"offset": "2px",
						"style": "dotted",
						"width": "1px"
					},
					"border": {
						"color": "var(--wp--custom--color--interactive-hover)"
					}
				},
				"hover": {
					"color": {
						"background": "var(--wp--custom--color--interactive-hover)",
						"text": "var(--wp--custom--color--background)"
					},
					"border": {
						"color": "var(--wp--custom--color--interactive-hover)"
					}
				},
				"border": {
					"color": "var(--wp--custom--color--interactive)",
					"radius": "4px",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--custom--color--interactive)",
					"text": "var(--wp--custom--color--background)"
				},
				"spacing": {
					"padding": {
						"top": "0.667em",
						"bottom": "0.667em",
						"left": "1.333em",
						"right": "1.333em"
					},
					"borderPadding": {
						"bottom": "calc(var(--wp--custom--button--spacing--padding--bottom) - var(--wp--custom--button--border--width))",
						"left": "calc(var(--wp--custom--button--spacing--padding--left) - var(--wp--custom--button--border--width))",
						"right": "calc(var(--wp--custom--button--spacing--padding--right) - var(--wp--custom--button--border--width))",
						"top": "calc(var(--wp--custom--button--spacing--padding--top) - var(--wp--custom--button--border--width))"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "normal",
					"lineHeight": 1.2
				},
				"outline": {
					"spacing": {
						"borderPadding": {
							"bottom": "calc(var(--wp--custom--button--spacing--padding--bottom) - var(--wp--custom--button--outline--border--width))",
							"left": "calc(var(--wp--custom--button--spacing--padding--left) - var(--wp--custom--button--outline--border--width))",
							"right": "calc(var(--wp--custom--button--spacing--padding--right) - var(--wp--custom--button--outline--border--width))",
							"top": "calc(var(--wp--custom--button--spacing--padding--top) - var(--wp--custom--button--outline--border--width))"
						}
					},
					"border": {
						"width": "2px",
						"color": "var(--wp--custom--color--interactive)"
					},
					"text": {
						"color": "var(--wp--custom--color--interactive)"
					}
				}
			},
			"form": {
				"padding": ".5em 1em",
				"border": {
					"color": "var(--wp--custom--color--ui-border)",
					"radius": "0",
					"style": "solid",
					"width": "1px"
				},
				"checkbox": {
					"sizing": {
						"height": "16px",
						"width": "16px"
					},
					"checked": {
						"content": "url(assets/svg/check.svg)",
						"fontSize": "var(--wp--preset--font-size--x-small)",
						"position": {
							"left": "3px",
							"top": "3px"
						}
					},
					"unchecked": {
						"content": "\"\"",
						"position": {
							"left": "0",
							"top": "0.2em"
						}	
					}
				},
				"color": {
					"background": "transparent",
					"boxShadow": "none",
					"text": "inherit"
				},
				"label": {
					"spacing": {
						"margin": {
							"bottom": "var(--wp--preset--spacing--10)",
							"top": 0
						}
					},
					"typography": {
						"fontSize": "var(--wp--preset--font-size--small)",
						"fontWeight": "normal",
						"letterSpacing": "inherit",
						"textTransform": "none"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "1.1"
				}
			},
			"figcaption": {
				"fontSize": "var(--wp--preset--font-size--small)",
				"fontStyle": "italic",
				"fontWeight": "normal",
				"margin": "var(--wp--preset--spacing--10) 0 0 0"
			},
			"body": {
				"typography": {
					"lineHeight": 1.6,
					"fontWeight": 400
				}
			},
			"heading": {
				"typography": {
					"fontWeight": 400,
					"lineHeight": 1.125
				}
			},
			"subheading": {
				"typography": {
					"fontWeight": 600,
					"lineHeight": 1.125
				}
			},
			"code": {
				"typography": {
					"fontFamily": "monospace",
					"fontWeight": 500,
					"lineHeight": 1.125
				}
			},
			"layout": {
				"narrowSize" : "600px",
				"sidebarWidthSmall": "80%",
				"sidebarWidthMid": "60%"
			},
			"gap": {
				"baseline": "var(--wp--preset--spacing--20)",
				"horizontal": "var(--wp--preset--spacing--20)",
				"vertical": "var(--wp--preset--spacing--20)"
			},
			"navigation": {
				"item": {
					"padding": "0 var(--wp--preset--spacing--10)",
					"textDecoration": "none"
				},
				"link": {
					"padding": "var(--wp--preset--spacing--10) 0"
				},
				"submenu": {
					"border": {
						"color": "transparent",
